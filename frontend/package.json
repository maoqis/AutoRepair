{
  "name": "AutoRepair",
  "version": "1.0.0",
  "description": "An Auto Repair Management Tool",
  "main": "app/main.jsx",
  "scripts": {
    "precommit": "npm run lint",
    "lint": "eslint --cache --ignore-path .gitignore --ext .jsx,.js --format=node_modules/eslint-formatter-pretty --fix .",
    "test": "npm run lint",
    "build": "webpack",
    "start": "webpack-dev-server --devtool eval --progress --hot --content-base app",
    "deploy": "NODE_ENV=production webpack --config webpack.production.config.js",
    "deploy-windows": "SET NODE_ENV=production & webpack --config webpack.production.config.js",
    "validate": "npm ls"
  },
  "dependencies": {
    "babel-runtime": "6.x",
    "base-64": "^0.1.0",
    "es6-promise": "^4.1.1",
    "react": "15.x",
    "react-bootstrap": "^0.31.3",
    "react-dom": "15.x",
    "react-router-dom": "^4.2.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "6.x",
    "babel-eslint": "7.x",
    "babel-loader": "6.x",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-0": "6.x",
    "copy-webpack-plugin": "latest",
    "css-loader": "~0.26.1",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-compat": "^1.0.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "latest",
    "exports-loader": "^0.6.4",
    "husky": "^0.12.0",
    "imports-loader": "^0.7.1",
    "open-browser-webpack-plugin": "0.0.3",
    "style-loader": "~0.13.1",
    "webpack": "2.x",
    "webpack-dev-server": "2.x"
  },
  "keywords": [
    "es6",
    "web app",
    "Auto repair"
  ],
  "author": "Sabbir Ahmed",
  "license": "MIT",
  "config": {}
}
